    transform:scaleY(1);}
  38%{top:-200px;
    right:369px;
    transform:scaleY(16);
    transform:scaleY(16);
    border-radius:0px;}
  44%{top:70px;
    right:369px;
    transform:scaleY(2);
    transform:scaleY(2);
    border-radius:6px;}
  46%{top:81px;
    right:369px;
    transform:scaleY(1);
    transform:scaleY(1);}
  55%{top:81px;
    right:123px;}
  59%{top:160px;
    right:123px;}
  61%{top:182px;
    right:150px;}
  62%{top:192px;
    right:150px;}
  65%{top:216px;
    right:122px;}
  66%{top:226px;
    right:122px;}
  67%{top:234px;
    right:135px;}
  68%{top:226px;
    right:149px;}
  69%{top:215px;
    right:149px;}
  71%{top:193px;
    right:124px;}
  72%{top:183px;
    right:124px;}
  74%{top:160px;
    right:151px;}
  83%{top:49px;
    right:151px;}
  87%{top:49px;
    right:98px;}
  92%{top:122px;
    right:98px;}
  96%{top:122px;
