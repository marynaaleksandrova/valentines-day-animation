@import 'nib'

body {
  height:100%;
  background: #940268; /* Old browsers */
  background: url(images/bg_heart.png) 50% 0 no-repeat, -moz-radial-gradient(center, ellipse cover,  #940268 0%, #66004d 100%); /* FF3.6+ */
  background: url(images/bg_heart.png) 50% 0 no-repeat, -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,#940268), color-stop(100%,#66004d)); /* Chrome,Safari4+ */
  background: url(images/bg_heart.png) 50% 0 no-repeat, -webkit-radial-gradient(center, ellipse cover,  #940268 0%,#66004d 100%); /* Chrome10+,Safari5.1+ */
  background: url(images/bg_heart.png) 50% 0 no-repeat, -o-radial-gradient(center, ellipse cover,  #940268 0%,#66004d 100%); /* Opera 12+ */
  background: url(images/bg_heart.png) 50% 0 no-repeat, -ms-radial-gradient(center, ellipse cover,  #940268 0%,#66004d 100%); /* IE10+ */
  background: url(images/bg_heart.png) 50% 0 no-repeat, radial-gradient(center, ellipse cover,  #940268 0%,#66004d 100%); /* W3C */
  background-size: 100%;
  overflow:hidden;
}
#wrapper{
  width:1000px;
  height:700px;
  margin:0 auto;
  position:relative;
}
#people_wrapper{
  width:1000px;
  position:absolute;
  bottom:0;
  height:81px;
}
#first_person{
  position:absolute;
  bottom:0;
  left:0;
  width:38px;
  height:81px;
  opacity:0;
  animation: move_first_person 3000ms linear 1;
}
#boy_body{
  position:absolute;
  top:0;
}
#boy_leg1{
  position: absolute;
  bottom:0;
  left: 16px;
  animation: move_leg1 300ms linear 600ms 4;
}
#boy_leg2{
  position: absolute;
  bottom:0;
  left: 16px;
  animation: move_leg2 300ms linear 600ms 4;
}
#boy_hand{
  position: absolute;
  top: 35px;
  left: 14px;
  animation: move_hand 300ms linear 600ms 4;
}
#second_person{
  position:absolute;
  bottom:0;
  right:0;
  width:35px;
  height:78px;
  opacity:0;
  animation: move_second_person 3000ms linear 1;
}
#girl_body{
  position:absolute;
  top:0;
}
#girl_leg1{
  position: absolute;
  bottom:0;
  right: 16px;
  animation: move_leg1 300ms linear 600ms 6;
}
#girl_leg2{
  position: absolute;
  bottom:0;
  right: 16px;
  animation: move_leg2 300ms linear 600ms 6;
}
#girl_hand{
  position: absolute;
  top: 35px;
  right: 14px;
  animation: move_hand 300ms linear 600ms 6;
}
#ball_wrapper {
  width:602px;
  height:439px;
  margin:0 auto;
  position:absolute;
  z-index:1;
  bottom:0;
  right:199px;
}
#second_person_ball , #first_person_ball{
  width:12px;
  height:12px;
  border-radius: 6px;
}
#second_person_ball {
  background-color:#FF11FF;
  position: absolute;
  right: 115px;
  bottom: 90px;
  animation: move_second_person_ball 7000ms ease-in 3000ms 1;
}
#first_person_ball {
  background-color:#42BFBF;
  position: absolute;
  left: 22px;
  bottom: 103px;
  animation: move_first_person_ball 7000ms ease-in 3000ms 1;
}
#valentine_ball {
  background-color:#FF02A0;
  position:absolute;
  top: 165px;
  right: 255px;
  animation: move_valentine_ball 8000ms ease-in 10000ms 1;
}
#full_system {
  width:602px;
  height:439px;
  background:url(images/system_full.png) top left no-repeat;
  position:relative;
  z-index:2;
}
#full_system img#flask {
  position: relative;
  top: 146px;
  left: 312px;
  animation: flask_shake 8000ms ease-in 10000ms 1;
}
#full_system div.clamp{
  position:absolute;
  width:15px;
  height:35px;
  background-color:#828282;
  border:1px solid black;
}
#full_system div.clamp:nth-child(2){
  top:200px;
  right:39px;
  animation: clamp_first 7000ms ease-in 3000ms 1;
}
#full_system div.clamp:last-child{
  top: 200px;
  right: 81px;
  animation: clamp_last 7000ms ease-in 3000ms 1;
}
img#thermometer{
  position:absolute;
  top: 106px;
  left: 206px;
}
div#fire_stand{
  position:absolute;
  bottom:0;
  left:199px;
  animation: fire_move 8000ms ease-in 1;
}
div#fire_stand img:first-child{
  position:absolute;
  bottom:0;
}
img#fire{
  position:absolute;
  bottom:56px;
  left:-2px;
  animation: fire 1000ms ease-in infinite;
}
#final{
  position:absolute;
  top: 245px;
  right: 205px;
  transform: scale(0.1);
  transform: scale(0.1);
  animation: final 7000ms ease-in 17000ms 1;
}

/*-----Animations-----*/
/*-----Move girl ball-----*/
@keyframes move_second_person_ball {
  0% {
    right:115px;
    bottom:90px;
  }
