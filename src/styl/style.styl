  74%{left:137px;
    bottom:349px;
    transform:scale(1);
    opacity:1;}
  76%{left:137px;
    bottom:375px;}
  91%{left:325px;
    bottom:375px;}
  98%{left:325px;
    bottom:283px;}
  100%{left:335px;
    bottom:267px;}
}

/*-----Move valentine ball-----*/
@keyframes move_valentine_ball {
  0% {
    top:165px;
    right:255px;
    width:12px;
    height:12px;
    border-radius: 6px;
    transform:scale(0.2)
  }
