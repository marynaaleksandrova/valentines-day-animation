/*-----Flask shake-----*/
@keyframes flask_shake {
  0% {
    top: 146px;
    left: 312px;
    transform: rotate(0deg);
    transform-origin:50% 20%;
    transform: rotate(0deg);
    transform-origin:50% 20%;
  }
  6%{top: 146px;
    left: 312px;
    transform: rotate(0deg);
    transform-origin:50% 20%;
    transform: rotate(0deg);
    transform-origin:50% 20%;}
  8%{top: 146px;
    left: 312px;
    transform: rotate(45deg);
    transform-origin:50% 20%;
    transform: rotate(45deg);
    transform-origin:50% 20%;}
  10%{top: 146px;
    left: 312px;
    transform: rotate(-45deg);
    transform-origin:50% 20%;
    transform: rotate(-45deg);
    transform-origin:50% 20%;}
  12%{top: 146px;
    left: 312px;
    transform: rotate(45deg);
    transform-origin:50% 20%;
    transform: rotate(45deg);
    transform-origin:50% 20%;}
  14%{top: 146px;
    left: 312px;
    transform: rotate(0deg);
    transform-origin:50% 20%;
    transform: rotate(0deg);
    transform-origin:50% 20%;}
  16%{top: 146px;
    left: 312px;
    transform: rotate(90deg);
    transform-origin:50% 20%;
    transform: rotate(90deg);
    transform-origin:50% 20%;}
  18%{top: 146px;
    left: 312px;
    transform: rotate(90deg);
    transform-origin:50% 20%;
    transform: rotate(90deg);
    transform-origin:50% 20%;}
  100%{top: 146px;
    left: 312px;
    transform: rotate(0deg);
    transform-origin:50% 20%;
    transform: rotate(0deg);
    transform-origin:50% 20%;}
}

/*-----Clamps-----*/
@keyframes clamp_first {
  0% {
    top:200px;
    right:39px;
  }
  45%{top:200px;
    right:39px;}
  48%{top:200px;
    right:52px;}
  53%{top:200px;
    right:39px;}
  100%{top:200px;
    right:39px;}
}

@keyframes clamp_last {
  0%{
    top: 200px;
    right: 81px;
  }
  45%{top: 200px;
    right: 81px;}
  48%{top:200px;
    right:68px;}
  53%{top: 200px;
    right: 81px;}
  100%{top: 200px;
    right: 81px;}
}

/*-----Fire-----*/
@keyframes fire {
  0%{
    transform: scale(0.9, 1);
    transform: scale(0.9, 1);
  }
  15%{transform: scale(1, 1.1);
    transform: scale(1, 1.1);}
  50%{transform: scale(0.9, 1);
    transform: scale(0.9, 1);}
  85%{transform: scale(1, 1.1);
    transform: scale(1, 1.1);}
  100%{transform: scale(0.9, 1);
    transform: scale(0.9, 1);}
}

@keyframes fire_move {
  0% {
    bottom:0;
    left:117px;
  }
  80%{
    bottom:0;
    left:117px;
  }
  95%{
    bottom:0;
    left:199px;
  }
  100%{
    bottom:0;
    left:199px;
  }
}

/*-----Move legs-----*/
@keyframes move_leg1 {
  from {
    transform: rotate(45deg);
    transform-origin:50% 10%;
    transform: rotate(45deg);
    transform-origin:50% 10%;
  }
  to{
    transform: rotate(-45deg);
    transform-origin:50% 10%;
    transform: rotate(-45deg);
    transform-origin:50% 10%;
  }
}

@keyframes move_leg2 {
  from {
    transform: rotate(-45deg);
    transform-origin:50% 10%;
    transform: rotate(-45deg);
    transform-origin:50% 10%;
  }
  to {
    transform: rotate(45deg);
    transform-origin:50% 10%;
    transform: rotate(45deg);
    transform-origin:50% 10%;
  }
}
/*-----Move hand-----*/
@keyframes move_hand {
  from {
    transform: rotate(-20deg);
    transform-origin:50% 10%;
    transform: rotate(-20deg);
    transform-origin:50% 10%;
  }
  to{
    transform: rotate(40deg);
    transform-origin:50% 10%;
    transform: rotate(40deg);
    transform-origin:50% 10%;}
}

/*-----Move people-----*/
@keyframes move_first_person {
  0% {
    left:0;
    bottom:0;
  }
  10%{
    left:0;
    bottom:0;
    opacity:0;}
  20%{
    left:0;
    bottom:0;
    opacity:1;}
  60%{
    left:205px;
    bottom:0;
    opacity:1;}
  80%{
    left:205px;
    bottom:0;
    opacity:1;
    transform: scaleY(1);
    transform: scaleY(1);}
  90%{
    left:205px;
    bottom:20px;
    opacity:1;
    transform: scaleY(1.2);
    transform: scaleY(1.2);}
  100%{
    left:205px;
    bottom:50px;
    opacity:1;
    transform: scaleY(0.1);
    transform: scaleY(0.1);}
}

@keyframes move_second_person {
  0% {
    right:0;
    bottom:0;
  }
  10%{
    right:0;
    bottom:0;
    opacity:0;}
  20%{
    right:0;
    bottom:0;
    opacity:1;}
  80%{
    right:302px;
    bottom:0;
    opacity:1;
    transform: scaleY(1);
    transform: scaleY(1);}
  90%{
    right:302px;
    bottom:20px;
    opacity:1;
    transform: scaleY(1.2);
    transform: scaleY(1.2);}
  100%{
    right:302px;
    bottom:50px;
    opacity:1;
    transform: scaleY(0.1);
    transform: scaleY(0.1);}
}

/*-----Final-----*/
@keyframes final {
  0% {
    top:245px;
    right:205px;
    transform: scale(0.1);
    transform: scale(0.1);
  }
  50% {
    top:195px;
    right:195px;
    transform: scale(1);
    transform: scale(1);
  }
  100%{
    top:-100px;
    right:0;
    transform: scale(1);
    transform: scale(1);}
}